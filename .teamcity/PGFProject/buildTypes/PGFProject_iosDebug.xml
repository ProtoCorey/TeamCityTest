<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>PGFProject_iosDebug</name>
  <description>This is a pipeline to do PGFProject builds as set up in the build config from PGFProject Default</description>
  <settings ref="DailyBuildTemplate">
    <disabled-settings>
      <setting-ref ref="RUNNER_3" />
    </disabled-settings>
    <parameters>
      <param name="branch" value="main" />
      <param name="buildProfile" value="iosDebug" />
      <param name="target" value="iOS" />
    </parameters>
    <build-runners order="RUNNER_2, RUNNER_3, RUNNER_9, RUNNER_1, RUNNER_7">
      <runner id="RUNNER_3" name="Build" type="unity">
        <parameters>
          <param name="arguments" value="-username %unityUsername% -password %unityPassword% -executeMethod %executionMethod% -serial %unitySerial% -keystorePath %keystorePath% -keystorePass %keystorePassword% -buildPath  %buildPath% -buildNumber %build.number% -buildProfile %buildProfile%" />
          <param name="buildTarget" value="iOS" />
          <param name="noGraphics" value="true" />
          <param name="projectPath" value="%teamcity.build.checkoutDir%" />
          <param name="teamcity.step.mode" value="default" />
          <param name="unityVersion" value="2020.3." />
        </parameters>
      </runner>
      <runner id="RUNNER_9" name="Manual Unity Build" type="simpleRunner">
        <parameters>
          <param name="script.content" value="&quot;C:\Program Files\Unity\Hub\Editor\2020.3.18f1\Editor\Unity.exe&quot; -quit -batchmode -nographics -buildTarget %target% -projectPath %teamcity.build.checkoutDir% -executeMethod %executionMethod% -buildNumber %build.number% -buildProfile %buildProfile% -buildPath %buildPath% -keystorePass %keystorePassword% -keystorePath %keystorePath% -logfile" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

