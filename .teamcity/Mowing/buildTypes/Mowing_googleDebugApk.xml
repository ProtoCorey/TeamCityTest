<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Mowing_googleDebugApk</name>
  <description>This is a pipeline to do Mowing builds as set up in the build config from Mowing Default</description>
  <settings ref="UnityBuildTemplate">
    <parameters>
      <param name="branch" value="main/Release1.18.0/HALLOWEEN" />
      <param name="buildProfile" value="googleDebugApk" />
      <param name="target" value="Android" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_10" name="Unity Build (Command Line)" type="simpleRunner">
        <parameters>
          <param name="script.content" value="&quot;%unityDirectory%Hub\Editor\%unityVersion%\Editor\Unity.exe&quot; -quit -batchmode -nographics -buildTarget %target% -projectPath %teamcity.build.checkoutDir% -executeMethod %executionMethod% -buildNumber %build.number% -buildProfile %buildProfile% -buildPath %buildPath% -keystorePass %keystorePassword% -keystorePath %keystorePath% -logfile" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

